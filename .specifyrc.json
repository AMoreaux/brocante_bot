{
  "repository": "@specifyapp/r",
  "head": "set-up-specify",
  "base": "master",
  "rules": [
 {
      "name": "Design Tokens / orlos",
      "path": "./output/styles/colors.css",
      "filter": {
        "types": [
          "color"
        ]
      },
      "parsers": [
        {
          "name": "sort-by",
          "options": {
            "keys": [
              "name"
            ]
          }
        },
        {
          "name": "to-css-custom-properties",
          "options": {
            "formatName": "kebabCase"
          }
        }
      ]
    }
  ]
}
